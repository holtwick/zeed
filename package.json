  },
  "homepage": "https://github.com/holtwick/zeed",
  "repository": {
    "type": "git",
    "url": "https://github.com/holtwick/zeed.git"
  },
  "bugs": {
    "url": "https://github.com/holtwick/zeed/issues"
  },
  "keywords": [
    "foundation",
    "typescript",
    "eventemitter",
    "logging",
    "kit",
    "lib",
    "utils",
    "zeed"
  ],
  "sideEffects": true,
  "exports": {
    ".": {
      "types": "./dist/index.all.d.ts",
      "browser": "./dist/index.browser.js",
      "require": "./dist/index.node.cjs",
      "node": "./dist/index.node.js",
      "default": "./dist/index.browser.js"
    }
  },
  "main": "dist/index.node.cjs",
  "module": "dist/index.browser.js",
  "types": "dist/index.all.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.13.1"
  },
  "scripts": {
    "build": "nr clean && nr build:tsup",
    "build:tsup": "tsup src/index.all.ts --dts-only && tsup src/index.browser.ts src/index.node.ts --format esm,cjs --dts --sourcemap --minify --keep-names",
    "check": "tsc --noEmit -p tsconfig.json",
    "clean": "rm -rf dist",
    "coverage": "vitest --run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build && nr test:publish",
    "start": "nr watch",
    "test": "vitest",
    "test:publish": "vitest --run",
    "watch": "nr build:tsup -- --watch"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@antfu/ni": "^0.18.2",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.13",
    "c8": "^7.12.0",
    "cross-fetch": "^3.1.5",
    "esbuild": "^0.15.5",
    "eslint": "^8.24.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vitest": "^0.22.1"
  }
}
